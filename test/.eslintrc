{
  "extends": "../.eslintrc",
  "env": {
    "mocha": true
  },
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false,
    "expect": false,
    "fail": false,
    "should": false
  },
  "rules": {
    "no-console": [
      "off"
    ],
    "no-magic-numbers": [
      "off"
    ],
    "no-new": [
      "off"
    ],
    "no-process-env": [
      "off"
    ],
    "no-unused-expressions": [
      "off"
    ],
    "max-lines": [
      "off"
    ],
    "max-nested-callbacks": [
      "off"
    ],
    "newline-per-chained-call": [
      "off"
    ],
    "prefer-arrow-callback": [
      "off"
    ]
  }
}